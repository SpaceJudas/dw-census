      "relevance": "0.487867",
      "quotations": [
        {
          "quotation": "\n\"Anyway,\" said Magrat, \"you killed that horrid man!\"",
          "sentiment": {
            "score": "-0.451568",
            "type": "negative"
          }
        },
        {
